/* pages/match/match.wxss */
.container{
  overflow: hidden;
}
.checkbox{
  padding: 14rpx;
  width: 25%;
  box-sizing: border-box;
  float: left;
  text-align: center;
}
.label-wrapper{
  display: block;
  position: relative;
  border: 2rpx solid #ccc;
}
.exp-avatar{
  width: 100%;
  height: 166rpx;
  display: block;
}
.expdetail{
  padding: 20rpx 0;
}
.exp-name{
  display: block;
  height: 36rpx;
  line-height: 36rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.check-btn{
  position: absolute;
  bottom: 0;
  right: 0;
}
checkbox .wx-checkbox-input{
  margin-right: 0;
}
.right-con{
  float: right;
}
.picker{
  padding:8rpx 24rpx 20rpx;
  background: #fff;
}
.picker-main{
  padding: 24rpx 10rpx;
  border: 1px solid #eee;
  border-radius: 10rpx;
  position: relative;
}
.right-con{
  margin-right: 48rpx;
}
.arrow{
   top: 30rpx; 
}